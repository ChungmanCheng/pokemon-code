{
    "name": "pokemon-code-2",
    "displayName": "pokemon-code-2",
    "description": "Catch Pokemon in visual code",
    "version": "2.0.0",
    "icon": "imgs/pokeball.png",
    "publisher": "chungmancheng",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.showPokemon",
            "title": "Pokemon Code - Pokedex"
        },{
            "command": "extension.showInventory",
            "title": "Pokemon Code - Inventory"
        }],
        "viewsContainers": {
            "activitybar": [
            {
                "id": "package-explorer",
                "title": "Pokedex",
                "icon": "imgs/pokeball.png"
            }
            ]
        },
        "views": {
        "package-explorer": [
        {
            "id": "package-pokedex",
            "name": "Pokedex"
        },
        {
            "id": "package-inventory",
            "name": "Inventory"
        }
        ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}